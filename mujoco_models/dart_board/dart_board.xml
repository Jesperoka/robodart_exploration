<mujoco>
    <asset>
        <!-- Define the texture for the dartboard using a PNG image. -->
        <texture type="2d" name="dartboard_texture" file="dart_board/dartboard.png"/>
        <material name="dartboard_material" texture="dartboard_texture" />
    </asset>
    
    <worldbody>
        <!-- Dartboard placed relative to the parent body (floor_top_anchor) -->
        <body name="dartboard_body" pos="0 -2.37 1.13" quat="0 0 0.7071068 0.7071068">
            <geom type="cylinder" name="dartboard_geom" size="0.2285 0.005" material="dartboard_material" rgba="1 1 1 1"/>
        </body>
    </worldbody>
</mujoco>

