    <asset>
        <!-- Dart's visual representation from .obj file -->
        <mesh file="../../dart/assets/dart1.obj" name="dart_mesh"/>
    </asset>

    <worldbody>
        <!-- Dart body -->
        <body name="dart" pos="0 0 1.73">
            <!-- <joint type="free"/> -->

            <!-- Inertial properties (precomputed) -->
