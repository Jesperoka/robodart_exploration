<mujoco model="dartboard_model">
    <asset>
        <!-- Dartboard's visual representation from .obj file -->
        <mesh file="../../dart/assets/dart_board1.obj" name="dartboard_mesh"/>
        <!-- <mesh file="../../dart/assets/dart_board.mtl" name="dartboard_material"/> -->
    </asset>

    <worldbody>
        <!-- Dartboard body -->
        <body name="dartboard" pos="0 1 0"> <!-- Positioned 1 unit along the z-axis as an example -->
            <geom type="mesh" mesh="dartboard_mesh" rgba="-1 -1 -1 1" contype="0" conaffinity="1"/> <!-- Assuming white color for simplicity -->
        </body>
    </worldbody>
</mujoco>
